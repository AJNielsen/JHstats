{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ImageDomainConfiguration": {
    "DomainList": [ "pic.twitter.com", "instagram.com", "imgur.com" ]
  },
  "TwitterConfiguration": {
    "BaseUrl": "https://api.twitter.com",
    "BearerToken": "YOUR_TWITTER_BEARER_TOKEN_HERE"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "YOUR_APPLICATION_INSIGHTS_KEY_HERE"
  }
}